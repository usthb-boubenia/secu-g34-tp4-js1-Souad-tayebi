import multiprocessing
import time

def task(seconds, name): 
    print(f"{name} starting")
    time.sleep(seconds)
    print(f"{name} finished")
    return f"{name} done after {seconds}s"

if __name__ == "__main__":

    p1 = multiprocessing.process(target=task, args=(2, "Process-1"))
    p2 = multiprocessing.process(target=task, args=(2, "Process-2"))

    p1.start()
    p2.start()

    p1.join()
    p2.join()

print("main program continues")